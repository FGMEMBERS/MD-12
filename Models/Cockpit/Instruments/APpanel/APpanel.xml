<!-- 777-200
Justin Smithies
Syd Adams
Autopilot Control Panel
-->

<PropertyList>

<path>APpanel.ac</path>

    <panel>
        <path>Aircraft/MD-12/Models/ap2dpanel.xml</path>
        <bottom-left>
            <x-m>0.004</x-m>
            <y-m>-0.160</y-m>
            <z-m>0.010</z-m>
        </bottom-left>
        <bottom-right>
            <x-m>0.004</x-m>
            <y-m>0.160</y-m>
            <z-m>0.010</z-m>
        </bottom-right>
        <top-left>
            <x-m>0.004</x-m>
            <y-m>-0.160</y-m>
            <z-m>0.030</z-m>
        </top-left>
    </panel>

<!--  Upper Panel Autopilot Controls-->

        <animation>
        <type>rotate</type>
        <object-name>BLimit.knob</object-name>
        <property>instrumentation/afds/settings/bank-max</property>
        <factor>-20.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.007</y-m>
            <z-m>-0.006</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

        <animation>
        <type>rotate</type>
        <object-name>IAS.knob</object-name>
        <property>instrumentation/afds/inputs/speed-src</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>1</ind><dep>122</dep></entry>
            <entry><ind>2</ind><dep>188</dep></entry>
            <entry><ind>3</ind><dep>239</dep></entry>
        </interpolation>
        <center>
            <x-m>0</x-m>
            <y-m>-0.133</y-m>
            <z-m>-0.018</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

        <animation>
        <type>rotate</type>
        <object-name>ALTmode.knob</object-name>
        <property>instrumentation/afds/inputs/alt-knob</property>
        <factor>100</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0.135</y-m>
            <z-m>-0.007</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FDtoggle</object-name>
        <property>instrumentation/afds/inputs/FD</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.207</y-m>
            <z-m>-0.020</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>THARM.switch</object-name>
        <property>instrumentation/afds/inputs/at-armed</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.179</y-m>
            <z-m>0.025</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>THARM1.switch</object-name>
        <property>instrumentation/afds/inputs/at-armed[1]</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.179</y-m>
            <z-m>0.025</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APtoggle</object-name>
        <condition>
            <not>
                <property>autopilot/locks/passive-mode</property>
            </not>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>HDGtoggle</object-name>
        <condition>
            <or>
                <equals>
                    <property>instrumentation/afds/ap-modes/roll-mode</property>
                    <value>HDG SEL</value>
                </equals>
                <equals>
                    <property>instrumentation/afds/ap-modes/roll-mode</property>
                    <value>HDG HOLD</value>
                </equals>
            </or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>LNAV.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/roll-mode</property>
                <value>LNAV</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ALTtoggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/pitch-mode</property>
                <value>ALT</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ATtoggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/speed-mode</property>
                <value>SPD</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>VNAV.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/pitch-mode</property>
                <value>VNAV</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>FLCH.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/speed-mode</property>
                <value>FLCH</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>LOC.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/roll-mode</property>
                <value>LOC</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APP.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/pitch-mode</property>
                <value>G/S</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>VS.toggle</object-name>
        <condition>
            <or>
                <equals>
                    <property>instrumentation/afds/ap-modes/pitch-mode</property>
                    <value>V/S</value>
                </equals>
                <equals>
                    <property>instrumentation/afds/ap-modes/pitch-mode</property>
                    <value>FPA</value>
                </equals>
            </or>
        </condition>
    </animation>


